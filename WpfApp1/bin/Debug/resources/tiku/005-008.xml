            <Solution xml:space="preserve" language="cpp" filename="61">#include &lt;iostream.h&gt;

long gcd(long a,long b) {
   if (b == 0) return a;
   return gcd(b,a%b);
}

int main(){
	long m,n;
	while(cin&gt;&gt;m&gt;&gt;n){
		cout&lt;&lt;gcd(m,n)&lt;&lt;endl;
	}
return 0;
}</Solution>
