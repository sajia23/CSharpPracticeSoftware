            <Solution xml:space="preserve" language="cpp" filename="a.cpp">#include&lt;iostream.h&gt;
int isprime(int a)
{
	int i;
	for(i=2;i&lt;a;i++)
		if(a%i==0)
			break;
	if(i==a)
		return 1;
	return 0;
}
int main()
{
	int n,a,i;
	cin&gt;&gt;n;
	while(n--)
	{
		cin&gt;&gt;a;
		for(i=2;i&lt;a;i++)
		{
			if(isprime(i)&amp;&amp;isprime(a-i))
				break;
		}
		cout&lt;&lt;i&lt;&lt;' '&lt;&lt;a-i&lt;&lt;endl;
	}
	return 0;
}
