            <Solution xml:space="preserve" language="cpp" filename="058">#include&lt;iostream.h&gt;  //
bool is7(int n){ //
    if(n%7==0) //
    	return true; //
	while(n&gt;0){ //
	    if(n%10==7) //
	    	return true; //
 		n/=10; //
	}
 	return false;    //
}    
int main(){   //
    int n;   //
    while(cin&gt;&gt;n){ //
        int i;  //
        bool first=true;//
        for(i=1;i&lt;=n;i++){ //
            if(is7(i)){ //
                if(!first) //
            	cout&lt;&lt;' '&lt;&lt;i; //
            	else  //
            	{ 
            	    cout&lt;&lt;i; //
            	    first=false; //
            	}    
         	}   	
        }
        cout&lt;&lt;endl;    //
    }    
    return 0; //
}    
